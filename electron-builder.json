{
    "productName": "demo",
    "appId": "com.demo.electron",
    "copyright": "@copyright",
    "extraMetadata": {
      "main": "mainPackage/index.js"
    },
    "directories": {
      "output": "dist"
    },
    "asar": true,
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "demo"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://xxxxx/"
      }
    ],
    "files": [
      "mainPackage/**/*",
      "webPackage/**/*"
    ],
    "win": {
      "icon": "build/icons/222.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "build/icons/222.ico",
      "maintainer": "gerry",
      "target": [
        {
          "target": "deb"
        }
      ]
    }
  }