    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "electron": "^10.1.1",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.14.0",
    "eslint-loader": "^4.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "node-sass": "^5.0.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
