{
  "name": "demo",
  "version": "1.0.0",
  "description": "This is a Electron demo!",
  "main": "./.electron/entry.js",
  "scripts": {
    "start-web": "webpack-dev-server --config ./build/webpack.config.js --mode development",
    "start-client": "chcp 65001 && electron . --mode development",
    "build-web": "webpack --config ./build/webpack.config.js --mode production",
    "build-dll": "webpack --config ./build/webpack.dll.config.js --mode production",
    "build-main": "webpack --config ./build/webpack.main.config.js --mode production",
    "build-win": "electron-builder -w --config ./electron-builder.json",
    "build-win-package": "npm run build-web && npm run build-main && npm run build-win",
    "build-arm": "electron-builder --arm64 --config ./electron-builder.json",
    "build-linux": "electron-builder --linux --config ./electron-builder.json",
    "test-storage": "nyc mocha .electron/test/storage.spec.js",
    "test-timer": "nyc mocha ./web/test/timeRecord.spec.js",
    "test-timer-reporter": "mocha ./web/test/timeRecord.spec.js --reporter mochawesome --reporter-options reportDir=mochaReporter,reportFilename=customReportFilename",
    "test-vue-demo": "mochapack  --webpack-config ./build/webpack.test.config.js --require ./web/test/setup.js web/test/demo.spec.js",
    "test-add": "nyc mocha .electron/test/calc.spec.js",
    "test-add-reporter": "mocha .electron/test/calc.spec.js --reporter mochawesome --reporter-options reportDir=mochaReporter,reportFilename=addReporter"
  },
  "keywords": [
    "electron"
  ],
  "author": "gerry",
  "email": "957282514@qq.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.14.2",
    "@babel/preset-env": "^7.12.7",
    "@vue/test-utils": "^1.2.2",
    "babel-loader": "^8.2.1",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "5.1.2",
    "css-loader": "3.6.0",
    "electron": "^10.1.1",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.1.1",
    "electron-log": "^4.4.0",
    "eslint": "^7.14.0",
    "eslint-loader": "^4.0.2",
    "esm": "^3.2.25",
    "expect.js": "^0.3.1",
    "file-loader": "^6.2.0",
    "generate-asset-webpack-plugin": "^0.3.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.3.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^16.6.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "1.6.2",
    "mocha": "^9.0.2",
    "mochapack": "^2.1.2",
    "mochawesome": "^6.2.2",
    "node-sass": "^5.0.0",
    "nyc": "^15.1.0",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-parallel-uglify-plugin": "1.1.2"
  },
  "dependencies": {
    "echarts": "^5.0.1",
    "element-ui": "^2.15.1",
    "minimist": "^1.2.5",
    "normalize.css": "^8.0.1",
    "pdfjs-dist": "^2.6.347",
    "regedit": "^4.0.0",
    "v-debounce-throttle": "^2.0.2",
    "vue": "^2.6.12",
    "vue-design": "0.0.5",
    "vue-router": "^3.4.9"
  }
}
